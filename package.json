{
    "name": "stocache",
    "version": "1.0.0",
    "description": "local storage cache",
    "main": "lib/index.js",
    "scripts": {
        "dev": "concurrently -n build,serve -k \"yarn dev:build\" \"yarn dev:serve\"",
        "dev:build": "webpack --config config/webpack.js --env development",
        "dev:serve": "node config/server.js",
        "build": "webpack --config config/webpack.js --env production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dalirnet/stocache.git"
    },
    "keywords": [
        "localstorage",
        "stocache",
        "storage",
        "cache",
        "iran"
    ],
    "author": "AmirRezaDalir",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dalirnet/stocache/issues"
    },
    "homepage": "https://github.com/dalirnet/stocache#readme",
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@webpack-cli/serve": "^1.2.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "live-server": "^1.2.1",
        "terser-webpack-plugin": "^5.0.3",
        "webpack": "^5.11.0",
        "webpack-cli": "4",
        "yargs": "^16.2.0"
    },
    "dependencies": {}
}
